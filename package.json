{
  "name": "react-js-rollup-library",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "generate-styles": "style-dictionary build --config style-dictionary.config.js",
    "build-sass": "sass --no-source-map ./src/styles/index.scss ./src/styles/sass-generated/index.css && postcss ./src/styles/sass-generated/index.css -o ./src/styles/postcss-processed/index.css",
    "watch-sass": "sass --no-source-map --watch ./src/styles/index.scss ./src/styles/sass-generated/index.css && postcss ./src/styles/sass-generated/index.css -o ./src/styles/postcss-processed/index.css --watch",
    "generate-icons-list": "node scripts/generate-icons-list.js",
    "rm-build": "rimraf build --glob",
    "rm-node_modules": "rimraf node_modules --glob",
    "rm-package-lock": "rimraf package-lock.json",
    "clean": "npm run rm-package-lock && npm run rm-node_modules",
    "lint-js": "eslint \"**/*.{js,jsx}\" --quiet --ignore-path .eslintignore",
    "lint-css": "stylelint \"**/*.{css,scss}\"",
    "prettier": "prettier",
    "lint": "npm run lint-js && npm run lint-css && npm run prettier -- --check .",
    "lint-js:fix": "npm run lint-js -- --fix",
    "lint-css:fix": "npm run lint-css -- --fix",
    "prettier:fix": "prettier --write --ignore-unknown",
    "lint:fix": "npm run lint-js:fix && npm run lint-css:fix && npm run prettier:fix -- .",
    "lint-staged": "npx lint-staged",
    "grant-permissions:husky": "chmod +x .husky/*",
    "prepare": "husky install &&  npm run grant-permissions:husky",
    "commit": "git-cz",
    "knip": "knip",
    "knip:debug": "knip --debug",
    "knip:check-files": "knip --include files",
    "knip:w/o-devDependencies": "knip --exclude devDependencies",
    "knip-prod": "knip --production",
    "knip-prod:check-files": "knip --production --include files",
    "knip:pre-commit": "npm run knip-prod:check-files"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.3",
    "knip": "^5.27.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "postcss-cli": "^11.0.0",
    "postcss-preset-env": "^9.6.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sass": "^1.77.8",
    "style-dictionary": "^4.0.1",
    "stylelint": "^14.16.1",
    "stylelint-config-clean-order": "^5.4.2",
    "stylelint-config-hudochenkov": "^8.0.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.0.0",
    "stylelint-order": "^6.0.4",
    "validate-branch-name": "^1.3.1"
  },
  "dependencies": {
    "@babel/core": "^7.24.9",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.0"
  }
}
